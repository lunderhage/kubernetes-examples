{
  "builders": [
    {
      "communicator": "ssh",
      "source_path": "ubuntu/focal64",
      "provider": "virtualbox",
      "add_force": true,
      "type": "vagrant",
      "box_name": "foctopal64_kubernetes"
    }
  ],
  "provisioners": [
  	{
  		"type": "shell",
  		"scripts": [ 
  			"install_docker.sh",
  			"install_kubernetes.sh"
  		]
  	},
  	{
  		"type": "shell",
  		"inline": "sudo apt-get install -y avahi-daemon libnss-mdns"
  	}
  ]
}